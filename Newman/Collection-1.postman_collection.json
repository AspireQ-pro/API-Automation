	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"packages": {},
				"requests": {},
				"exec": [
					"/************************************************************\r",
					" * FINAL SAFE PRE-REQUEST SCRIPT (NO OVER-SKIP)\r",
					" ************************************************************/\r",
					"\r",
					"/******** CSV INPUT ********/\r",
					"let csvIterationRaw = pm.iterationData.get(\"Iteration\");\r",
					"let csvApiName      = pm.iterationData.get(\"API_Request_Name\");\r",
					"\r",
					"/******** CURRENT CONTEXT ********/\r",
					"let currentIteration = pm.info.iteration + 1;\r",
					"let currentRequest   = pm.info.requestName;\r",
					"\r",
					"/******** VALIDATE CSV ITERATION ********/\r",
					"if (!csvIterationRaw) {\r",
					"    console.log(\"⚠️ CSV Iteration empty → allowing execution\");\r",
					"} else {\r",
					"    let csvIteration = Number(csvIterationRaw);\r",
					"\r",
					"    if (isNaN(csvIteration)) {\r",
					"        console.log(\"⚠️ CSV Iteration NaN → allowing execution\");\r",
					"    } else if (csvIteration !== currentIteration) {\r",
					"        console.log(`⏭️ Skip | CSV=${csvIteration} Current=${currentIteration}`);\r",
					"        pm.execution.skipRequest();\r",
					"        return;\r",
					"    }\r",
					"}\r",
					"\r",
					"/******** REQUEST NAME CONTROL ********/\r",
					"if (csvApiName && csvApiName !== currentRequest) {\r",
					"    console.log(`⏭️ Skip | CSV API=${csvApiName} Current=${currentRequest}`);\r",
					"    pm.execution.skipRequest();\r",
					"    return;\r",
					"}\r",
					"\r",
					"console.log(`✅ RUN | Iteration ${currentIteration} | ${currentRequest}`);\r",
					"\r",
					""
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"packages": {},
				"requests": {},
				"exec": [
					"/************************************************************\r",
					" * STEP 0 – STATUS CODE VALIDATION\r",
					" ************************************************************/\r",
					"let actualStatusCode   = pm.response.code;\r",
					"let expectedStatusCode = Number(pm.iterationData.get(\"Expected_Status_Code\"));\r",
					"\r",
					"pm.test(\"Validate Status Code\", () => {\r",
					"    pm.expect(\r",
					"        actualStatusCode,\r",
					"        `❌ Status Code Mismatch | Expected: ${expectedStatusCode}, Actual: ${actualStatusCode}`\r",
					"    ).to.eql(expectedStatusCode);\r",
					"});\r",
					"\r",
					"if (actualStatusCode !== expectedStatusCode) {\r",
					"    console.warn(\"❌ Status mismatch. Skipping body validation.\");\r",
					"    return;\r",
					"}\r",
					"\r",
					"/************************************************************\r",
					" * STEP 1 – READ RESPONSE\r",
					" ************************************************************/\r",
					"let response = pm.response.json();\r",
					"\r",
					"/************************************************************\r",
					" * STEP 2 – CSV INPUTS\r",
					" ************************************************************/\r",
					"let scenareo        = (pm.iterationData.get(\"Scenareo\") || \"\").split(\",\");\r",
					"let expected_result = JSON.parse(pm.iterationData.get(\"Expected_result\"));\r",
					"let uniqueKey       = pm.iterationData.get(\"Uniquekey\");\r",
					"let uniqueValue     = pm.iterationData.get(\"Uniquevalue\");\r",
					"\r",
					"/************************************************************\r",
					" * STEP 3 – FLATTEN EXPECTED JSON (DUPLICATE SAFE)\r",
					" ************************************************************/\r",
					"function extractExpectedPairs(obj, result = {}) {\r",
					"\r",
					"    if (obj === null || typeof obj !== \"object\") return result;\r",
					"\r",
					"    for (let [key, value] of Object.entries(obj)) {\r",
					"\r",
					"        if (value !== null && typeof value === \"object\" && !Array.isArray(value)) {\r",
					"            extractExpectedPairs(value, result);\r",
					"        } else {\r",
					"            if (result.hasOwnProperty(key)) {\r",
					"                if (!Array.isArray(result[key])) {\r",
					"                    result[key] = [result[key]];\r",
					"                }\r",
					"                result[key].push(value);\r",
					"            } else {\r",
					"                result[key] = value;\r",
					"            }\r",
					"        }\r",
					"    }\r",
					"    return result;\r",
					"}\r",
					"\r",
					"/************************************************************\r",
					" * STEP 4 – GET ACTUAL VALUES\r",
					" ************************************************************/\r",
					"function getActualValues(obj, keyToFind, uniqueKey, uniqueValue, values = []) {\r",
					"\r",
					"    if (!obj) return values;\r",
					"\r",
					"    if (Array.isArray(obj)) {\r",
					"        obj.forEach(item =>\r",
					"            getActualValues(item, keyToFind, uniqueKey, uniqueValue, values)\r",
					"        );\r",
					"        return values;\r",
					"    }\r",
					"\r",
					"    if (typeof obj === \"object\") {\r",
					"\r",
					"        if (\r",
					"            uniqueKey &&\r",
					"            obj.hasOwnProperty(uniqueKey) &&\r",
					"            String(obj[uniqueKey]) !== String(uniqueValue)\r",
					"        ) {\r",
					"            return values;\r",
					"        }\r",
					"\r",
					"        for (let [key, value] of Object.entries(obj)) {\r",
					"\r",
					"            if (key === keyToFind) {\r",
					"                values.push(value);\r",
					"            }\r",
					"\r",
					"            if (typeof value === \"object\" && value !== null) {\r",
					"                getActualValues(value, keyToFind, uniqueKey, uniqueValue, values);\r",
					"            }\r",
					"        }\r",
					"    }\r",
					"\r",
					"    return values;\r",
					"}\r",
					"\r",
					"/************************************************************\r",
					" * STEP 5 – ASSERTION (CLEAR FAILURE MESSAGES)\r",
					" ************************************************************/\r",
					"function assertKey(testName, key, expected, actualValues) {\r",
					"\r",
					"    pm.test(testName, () => {\r",
					"\r",
					"        // ❌ KEY MISSING\r",
					"        if (!actualValues || actualValues.length === 0) {\r",
					"            pm.expect.fail(\r",
					"                `❌ Key Missing | Key \"${key}\" not found anywhere in API response`\r",
					"            );\r",
					"        }\r",
					"\r",
					"        // ✅ EXPECTED = NULL\r",
					"        if (expected === null) {\r",
					"            pm.expect(\r",
					"                actualValues.some(v => v === null),\r",
					"                `❌ NULL Mismatch | Key \"${key}\" expected NULL but actual values were: ${JSON.stringify(actualValues)}`\r",
					"            ).to.be.true;\r",
					"            return;\r",
					"        }\r",
					"\r",
					"        // ✅ EXPECTED = NOT_NULL\r",
					"      // ✅ EXPECTED = NOT_NULL or BLANK\r",
					"if (\r",
					"    expected === \"\" ||\r",
					"    String(expected).toUpperCase() === \"NOT_NULL\" ||\r",
					"    (Array.isArray(expected) &&\r",
					"     expected.map(v => String(v).toUpperCase()).includes(\"NOT_NULL\"))\r",
					") {\r",
					"    pm.expect(\r",
					"        actualValues.some(v => v !== null),\r",
					"        `❌ NOT_NULL Mismatch | Key \"${key}\" expected NOT NULL but actual values were ${JSON.stringify(actualValues)}`\r",
					"    ).to.be.true;\r",
					"    return;\r",
					"}\r",
					"\r",
					"\r",
					"\r",
					"        // ✅ EXPECTED = ARRAY (Duplicate expected values)\r",
					"        if (Array.isArray(expected)) {\r",
					"\r",
					"            let actualStr   = actualValues.map(v => String(v));\r",
					"            let expectedStr = expected.map(v => String(v));\r",
					"\r",
					"            pm.expect(\r",
					"                expectedStr.some(ev => actualStr.includes(ev)),\r",
					"                `❌ Value Mismatch | Key \"${key}\" | Expected ANY of ${JSON.stringify(expectedStr)} but Actual values were ${JSON.stringify(actualStr)}`\r",
					"            ).to.be.true;\r",
					"\r",
					"            return;\r",
					"        }\r",
					"\r",
					"        // ✅ EXPECTED = SINGLE VALUE\r",
					"        pm.expect(\r",
					"            actualValues.map(v => String(v)),\r",
					"            `❌ Value Mismatch | Key \"${key}\" | Expected: \"${expected}\" but Actual values were: ${JSON.stringify(actualValues)}`\r",
					"        ).to.include(String(expected));\r",
					"    });\r",
					"}\r",
					"\r",
					"/************************************************************\r",
					" * STEP 6 – MAIN EXECUTION\r",
					" ************************************************************/\r",
					"let expectedPairs = extractExpectedPairs(expected_result);\r",
					"\r",
					"Object.keys(expectedPairs).forEach((key, index) => {\r",
					"\r",
					"    let expectedValue = expectedPairs[key];\r",
					"\r",
					"    let actualValues = getActualValues(\r",
					"        response,\r",
					"        key,\r",
					"        uniqueKey,\r",
					"        uniqueValue\r",
					"    );\r",
					"\r",
					"    let testName = scenareo[index] || `Validate ${key}`;\r",
					"\r",
					"    assertKey(testName, key, expectedValue, actualValues);\r",
					"\r",
					"    console.log(\"KEY           :\", key);\r",
					"    console.log(\"EXPECTED      :\", expectedValue);\r",
					"    console.log(\"ACTUAL VALUES :\", actualValues);\r",
					"    \r",
					"});\r",
					""
				]
			}
		}
	],
