	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"packages": {},
				"requests": {},
				"exec": [
					"\r",
					"\r",
					"/******** 1Ô∏è‚É£ READ VALUES FROM CSV ********/\r",
					"let csvIteration = pm.iterationData.get(\"Iteration\");          \r",
					"let csvApiName   = pm.iterationData.get(\"API_Request_Name\");  \r",
					"let runFlag      = pm.iterationData.get(\"Running_Flag\");      \r",
					"\r",
					"/******** 2Ô∏è‚É£ CURRENT EXECUTION CONTEXT ********/\r",
					"let currentIteration = pm.info.iteration + 1;   // Current iteration (1-based)\r",
					"let currentRequest   = pm.info.requestName;     // Current request name\r",
					"\r",
					"/******** 3Ô∏è‚É£ RUNNING FLAG CHECK ********/\r",
					"/*\r",
					"   If Running_Flag is not \"Yes\",\r",
					"   skip the request immediately\r",
					"*/\r",
					"if (!runFlag || runFlag.toLowerCase() !== \"yes\") {\r",
					"    console.log(\"‚è≠Ô∏è SKIP | Running_Flag =\", runFlag);\r",
					"    pm.execution.skipRequest();\r",
					"    return;\r",
					"}\r",
					"\r",
					"/******** 4Ô∏è‚É£ ITERATION CONTROL ********/\r",
					"/*\r",
					"   If Iteration is provided in CSV,\r",
					"   it must match the current iteration\r",
					"*/\r",
					"if (csvIteration && Number(csvIteration) !== currentIteration) {\r",
					"    console.log(\r",
					"        \"‚è≠Ô∏è SKIP | CSV Iteration =\", csvIteration,\r",
					"        \"| Current Iteration =\", currentIteration\r",
					"    );\r",
					"    pm.execution.skipRequest();\r",
					"    return;\r",
					"}\r",
					"\r",
					"/******** 5Ô∏è‚É£ REQUEST NAME CONTROL ********/\r",
					"/*\r",
					"   If API_Request_Name is provided in CSV,\r",
					"   it must exactly match the request name\r",
					"*/\r",
					"if (csvApiName && csvApiName !== currentRequest) {\r",
					"    console.log(\r",
					"        \"‚è≠Ô∏è SKIP | CSV API =\", csvApiName,\r",
					"        \"| Current Request =\", currentRequest\r",
					"    );\r",
					"    pm.execution.skipRequest();\r",
					"    return;\r",
					"}\r",
					"\r",
					"\r",
					""
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"packages": {},
				"requests": {},
				"exec": [
					"/************************************************************\r",
					" * STEP -1 : STATUS CODE CHECK\r",
					" ************************************************************/\r",
					"let actual_code = pm.response.code;\r",
					"let expected_code = Number(pm.iterationData.get(\"Expected_Status_Code\"));\r",
					"\r",
					"if (actual_code !== expected_code) {\r",
					"    pm.test(\"Verify status code is correct\", () => {\r",
					"        pm.response.to.have.status(expected_code);\r",
					"    });\r",
					"    return;\r",
					"}\r",
					"\r",
					"/************************************************************\r",
					" * STEP 0 ‚Äì READ RESPONSE & CSV\r",
					" ************************************************************/\r",
					"let actualRoot = pm.response.json();\r",
					"let expectedRoot = JSON.parse(pm.iterationData.get(\"Expected_result\") || {});\r",
					"let rawScenario = String(pm.iterationData.get(\"Scenareo\") || \"\").trim();\r",
					"\r",
					"let uniqueKey   = String(pm.iterationData.get(\"Uniquekey\") || \"\").trim();\r",
					"let uniqueValue = String(pm.iterationData.get(\"Uniquevalue\") || \"\").trim();\r",
					"\r",
					"/************************************************************\r",
					" * STEP 0.1 ‚Äì UNIQUE OBJECT PICK (FOR LIST APIs)\r",
					" ************************************************************/\r",
					"if (uniqueKey && uniqueValue) {\r",
					"\r",
					"    let foundObject = null;\r",
					"\r",
					"    function deepSearch(node) {\r",
					"        if (!node || foundObject) return;\r",
					"\r",
					"        if (Array.isArray(node)) {\r",
					"            node.forEach(deepSearch);\r",
					"            return;\r",
					"        }\r",
					"\r",
					"        if (typeof node === \"object\") {\r",
					"            if (\r",
					"                node.hasOwnProperty(uniqueKey) &&\r",
					"                String(node[uniqueKey]) === String(uniqueValue)\r",
					"            ) {\r",
					"                foundObject = node;\r",
					"                return;\r",
					"            }\r",
					"            Object.values(node).forEach(deepSearch);\r",
					"        }\r",
					"    }\r",
					"\r",
					"    deepSearch(actualRoot);\r",
					"\r",
					"    if (!foundObject) {\r",
					"        pm.test(\"Verify unique object exists\", () => {\r",
					"            pm.expect.fail(`‚ùå Object with ${uniqueKey}=${uniqueValue} not found`);\r",
					"        });\r",
					"        return;\r",
					"    }\r",
					"\r",
					"    // üî• Switch validation root\r",
					"    actualRoot = foundObject;\r",
					"}\r",
					"\r",
					"/************************************************************\r",
					" * STEP 1 ‚Äì GET VALUE BY PATH\r",
					" ************************************************************/\r",
					"function getValueByPath(obj, path) {\r",
					"    return path\r",
					"        .replace(/\\[(\\d+)\\]/g, '.$1')\r",
					"        .split('.')\r",
					"        .reduce((acc, key) => acc && acc[key], obj);\r",
					"}\r",
					"\r",
					"/************************************************************\r",
					" * STEP 2 ‚Äì FLATTEN EXPECTED OBJECT (path ‚Üí value)\r",
					" ************************************************************/\r",
					"function flattenExpected(obj, prefix = \"\", out = {}) {\r",
					"    for (let key in obj) {\r",
					"        let path = prefix ? `${prefix}.${key}` : key;\r",
					"        let val = obj[key];\r",
					"\r",
					"        if (typeof val === \"object\" && val !== null && !Array.isArray(val)) {\r",
					"            flattenExpected(val, path, out);\r",
					"        } else {\r",
					"            out[path] = val;\r",
					"        }\r",
					"    }\r",
					"    return out;\r",
					"}\r",
					"\r",
					"let expectedMap = flattenExpected(expectedRoot);\r",
					"let expectedKeys = Object.keys(expectedMap);\r",
					"\r",
					"/************************************************************\r",
					" * STEP 3 ‚Äì SMART AUTO SCENARIO NAME BUILDER\r",
					" ************************************************************/\r",
					"function buildScenarioName(path, expectedValue) {\r",
					"\r",
					"    let keyName = path.split(\".\").slice(-1)[0];\r",
					"    keyName = keyName.replace(/([A-Z])/g, \" $1\").toLowerCase();\r",
					"\r",
					"    if (\r",
					"        typeof expectedValue === \"string\" &&\r",
					"        expectedValue.startsWith(\"{{\") &&\r",
					"        expectedValue.endsWith(\"}}\")\r",
					"    ) {\r",
					"        let varName = expectedValue.slice(2, -2);\r",
					"        return `Capture ${keyName} into ${varName}`;\r",
					"    }\r",
					"\r",
					"    if (expectedValue === \"NOT_NULL\") {\r",
					"        return `Verify ${keyName} is not null`;\r",
					"    }\r",
					"\r",
					"    if (\r",
					"        typeof expectedValue === \"string\" &&\r",
					"        expectedValue.startsWith(\"REGEX:\")\r",
					"    ) {\r",
					"        return `Verify ${keyName} format is valid`;\r",
					"    }\r",
					"\r",
					"    if (expectedValue === null) {\r",
					"        return `Verify ${keyName} is null`;\r",
					"    }\r",
					"\r",
					"    return `Verify ${keyName} value is correct`;\r",
					"}\r",
					"\r",
					"/************************************************************\r",
					" * STEP 4 ‚Äì BUILD SCENARIO LIST (AUTO + CSV)\r",
					" ************************************************************/\r",
					"let userScenarios = rawScenario\r",
					"    ? rawScenario.split(\",\").map(s => s.trim())\r",
					"    : [];\r",
					"\r",
					"let scenarioList = expectedKeys.map((key, index) => {\r",
					"    return userScenarios[index] || buildScenarioName(key, expectedMap[key]);\r",
					"});\r",
					"\r",
					"/************************************************************\r",
					" * STEP 5 ‚Äì SCENARIO-WISE VALIDATION\r",
					" ************************************************************/\r",
					"expectedKeys.forEach((path, index) => {\r",
					"\r",
					"    let expectedValue = expectedMap[path];\r",
					"    let actualValue = getValueByPath(actualRoot, path);\r",
					"    let scenarioName = scenarioList[index];\r",
					"\r",
					"    pm.test(scenarioName, () => {\r",
					"\r",
					"        /* ---------- {{var}} ‚Üí NOT_NULL + SET VARIABLE ---------- */\r",
					"        if (\r",
					"            typeof expectedValue === \"string\" &&\r",
					"            expectedValue.startsWith(\"{{\") &&\r",
					"            expectedValue.endsWith(\"}}\")\r",
					"        ) {\r",
					"            let varName = expectedValue.slice(2, -2).trim();\r",
					"\r",
					"            pm.expect(\r",
					"                actualValue !== null &&\r",
					"                actualValue !== undefined &&\r",
					"                actualValue !== \"\",\r",
					"                `‚ùå ${path} expected NOT_NULL for {{${varName}}} but found ${actualValue}`\r",
					"            ).to.be.true;\r",
					"\r",
					"            pm.collectionVariables.set(varName, actualValue);\r",
					"            console.log(`‚úÖ Collection variable set: ${varName} = ${actualValue}`);\r",
					"            return;\r",
					"        }\r",
					"\r",
					"        /* ---------- NOT_NULL ---------- */\r",
					"        if (expectedValue === \"NOT_NULL\") {\r",
					"            pm.expect(\r",
					"                actualValue !== null &&\r",
					"                actualValue !== undefined &&\r",
					"                actualValue !== \"\",\r",
					"                `‚ùå ${path} expected NOT_NULL but found ${actualValue}`\r",
					"            ).to.be.true;\r",
					"            return;\r",
					"        }\r",
					"\r",
					"        /* ---------- REGEX ---------- */\r",
					"        if (\r",
					"            typeof expectedValue === \"string\" &&\r",
					"            expectedValue.startsWith(\"REGEX:\")\r",
					"        ) {\r",
					"            let pattern = expectedValue.replace(\"REGEX:\", \"\");\r",
					"            let regex = new RegExp(pattern);\r",
					"\r",
					"            pm.expect(\r",
					"                regex.test(String(actualValue)),\r",
					"                `‚ùå ${path} expected to match regex ${pattern} but got \"${actualValue}\"`\r",
					"            ).to.be.true;\r",
					"            return;\r",
					"        }\r",
					"\r",
					"        /* ---------- STRING CONTAINS ---------- */\r",
					"        if (typeof expectedValue === \"string\" && typeof actualValue === \"string\") {\r",
					"            pm.expect(\r",
					"                actualValue.includes(expectedValue),\r",
					"                `‚ùå ${path} expected to contain \"${expectedValue}\" but got \"${actualValue}\"`\r",
					"            ).to.be.true;\r",
					"            return;\r",
					"        }\r",
					"\r",
					"        /* ---------- DEFAULT EXACT MATCH ---------- */\r",
					"        pm.expect(\r",
					"            actualValue,\r",
					"            `‚ùå ${path} mismatch`\r",
					"        ).to.eql(expectedValue);\r",
					"    });\r",
					"});\r",
					""
				]
			}
		}
	],
